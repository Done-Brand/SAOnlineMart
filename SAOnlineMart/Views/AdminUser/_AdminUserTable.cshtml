@model IEnumerable<SAOnlineMart.Models.DataModels.AdminUser>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
